\s24\ql \li284\ri0\sb60\sa60\widctlpar\aspalpha\aspnum\faauto\adjustright\rin0\lin284\itap0 \f1\fs20\lang1033\langfe1029\cgrid\langnp1033\langfenp1029 {\cs25\super #}{ h_Maska}}}{ }{\cs25\super +{\footnote \pard\plain 
